        <!-- 定义函数的对应方法 -->  
        <function-signature>  
            java.lang.String tranTimeMonthDay(java.lang.String)  
        </function-signature>  
    </function>           
    <function>  
        <!-- 定义第3个函数：reverse -->  
        <name>tranCertSex</name>  
